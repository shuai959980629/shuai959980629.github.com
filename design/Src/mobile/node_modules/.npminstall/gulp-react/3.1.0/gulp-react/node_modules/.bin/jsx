#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../../react-tools/0.13.3/react-tools/bin/jsx" "$@"
  ret=$?
else 
  node  "$basedir/../../../../../react-tools/0.13.3/react-tools/bin/jsx" "$@"
  ret=$?
fi
exit $ret
